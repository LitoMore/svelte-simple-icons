{
  "name": "@icons-pack/svelte-simple-icons",
  "version": "4.0.0-rc.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "icons",
    "index.*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "description": "This package provides the Simple Icons packaged as a set of Svelte components.",
  "repository": "git@github.com:icons-pack/svelte-simple-icons.git",
  "author": "Jorge Luis Calleja <jorgelca1205@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "svelte",
    "simple-icons",
    "svelte-simple-icons",
    "icons",
    "svg",
    "inline",
    "design",
    "component",
    "codea"
  ],
  "scripts": {
    "clean": "rm -rf index.* package",
    "build": "pnpm clean && pnpm check && svelte-package && node ./scripts/clean_package.js && rm -rf package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean:icons": "rm -rf ./src/lib/icons/**",
    "generate:icons": "pnpm clean:icons && node ./scripts/generate-components.js",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/package": "^1.0.0",
    "tslib": "^2.4.1",
    "typescript": "5.2.2",
    "cross-env": "^7.0.2",
    "rimraf": "^3.0.2",
    "signale": "^1.4.0",
    "simple-icons": "9.19.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "uppercamelcase": "^3.0.0",
    "vite": "^4.0.5"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0",
    "svelte": "^3.54.0 || ^4.0.0"
  }
}
